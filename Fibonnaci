class Fibonnaci
{
	public static void main (String[] args) throws java.lang.Exception
	{
		System.out.println(returnFibonacciNumberUsingIndex(10));
	}

  public static int returnFibonacciNumberUsingIndex(int i){
      if(i <= 0) return 0;
      if(i == 1) return 1;
     return returnFibonacciNumberUsingIndex(i - 1) + returnFibonacciNumberUsingIndex(i - 2);
  }
}
